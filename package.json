{
  "name": "react-starter-app",
  "version": "1.0.0",
  "author": "Michael Sargent",
  "description": "boilerplate for static react applications",
  "private": true,
  "license": "MIT",
  "keywords": [
    "static",
    "react",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "homepage": "https://github.com/mksarge/react-starter-app",
    "bugs": "https://github.com/mksarge/react-starter-app/issues"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "browser-sync": "^2.18.8",
    "chai": "^4.0.0-canary.1",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "ejs": "^2.5.5",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.0",
    "firebase-tools": "^3.2.3",
    "front-matter": "^2.1.2",
    "highlight.js": "^9.9.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "markdown-it": "^8.2.2",
    "markdown-it-anchor": "^3.0.0",
    "markdown-it-table-of-contents": "^0.3.2",
    "mocha": "^3.2.0",
    "path-to-regexp": "^1.7.0",
    "pixrem": "^3.0.2",
    "pleeease-filters": "^3.0.1",
    "postcss": "^5.2.13",
    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^3.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-flexbugs-fixes": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.0",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nesting": "^2.3.1",
    "postcss-selector-matches": "^2.0.5",
    "postcss-selector-not": "^2.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "babel": {
    "presets": [
      "react",
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "eslint": "eslint app test utils run.js webpack.config.js",
    "stylelint": "stylelint \"app/**/*.css\" \"public/index.css\"",
    "lint": "npm run eslint; npm run stylelint",
    "test": "node_modules/.bin/_mocha --require test/.setup.js \"test/**/*.spec.js\"",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha --require test/.setup.js \"test/**/*.spec.js\"",
    "build": "node run build",
    "start": "node run",
    "publish": "npm run lint && node run publish"
  }
}
