{
  "name": "react-starter-app",
  "version": "1.0.0",
  "private": true,
  "description": "React Starter App is a static React/Redux boilerplate with modern build, test, and deploy tooling.",
  "author": "Michael Sargent <michael-sargent@hotmail.com> (https://mksarge.com)",
  "license": "MIT",
  "keywords": [
    "static",
    "react",
    "redux",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "homepage": "https://github.com/mksarge/react-starter-app",
    "bugs": "https://github.com/mksarge/react-starter-app/issues"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-json-router": "^0.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "browser-sync": "^2.18.12",
    "chai": "^4.0.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.4",
    "del": "^2.2.2",
    "ejs": "^2.5.6",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "firebase-tools": "^3.9.0",
    "front-matter": "^2.1.2",
    "highlight.js": "^9.11.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^10.1.0",
    "json-loader": "^0.5.4",
    "markdown-it": "^8.3.1",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-table-of-contents": "^0.3.2",
    "mocha": "^3.4.2",
    "path-to-regexp": "^1.7.0",
    "pixrem": "^3.0.2",
    "pleeease-filters": "^4.0.0",
    "postcss": "^6.0.1",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.0.1",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nesting": "^3.0.0",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.3.2",
    "style-loader": "^0.17.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "scripts": {
    "eslint": "eslint app test utils run.js webpack.config.js",
    "stylelint": "stylelint \"app/**/*.css\" \"public/index.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "node_modules/.bin/_mocha --require test/.setup.js \"test/**/*.spec.js\"",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha --require test/.setup.js \"test/**/*.spec.js\"",
    "ci": "npm run lint && npm run test",
    "build": "npm run ci && node run build",
    "start": "node run",
    "publish": "npm run ci && node run publish"
  }
}
